{
  "description": "Hybrid mode: Claude Code + Ollama execution (filesystem only)",
  "llm": {
    "provider": "ollama",
    "model": "qwen2.5-mcp",
    "baseUrl": "http://localhost:11434"
  },
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "D:/dev/MADF"],
      "env": {"ALLOWED_DIRECTORIES": "D:/dev/MADF"}
    }
  }
}